<h1>game form</h1>

<%= form_for game do |f| %>

  <div>
    <%= f.label :game_name  %>
    <%= f.text_field :game_name  %>
  </div>
  <div>
    <%= label_tag(name='creator nickname')  %>
    <%= text_field_tag 'creator_nickname'  %>
  </div>
  <div>
    <%= f.label :game_key  %>
    <%= f.text_field :game_key  %>
  </div>
  <div>
    <%= f.submit  %>
  </div>



<% end %>